			{123, 123, 139, 139, 123, 123, 139, 139, nil},
			{247, 139, 139, 139, 247, 139, 139, 139, nil},
			{nil, 302, 476, nil, nil, 302, 476, nil, nil},
			{247, 247, 247, 476, 249, 249, 249, 476, nil},
			{nil, 249, nil, 476, nil, 249, nil, 476, 883}
		}
	elseif (arena_current_level == "champion") then
		-- Waves for champion event
		waves = {
			{302, 302, 476, 476, 302, 302, 476, 476, nil},
			{571, 249, 249, 249, 571, 249, 249, 249, nil},
			{420, 476, 476, 476, 420, 476, 476, 476, nil},
			{nil, nil, 615, nil, nil, 420, 420, 420, nil},
			{nil, 249, nil, 476, nil, 249, nil, 476, 883},
			{493, 571, 493, 571, 493, 571, 493, 571, nil},
			{nil, 883, 249, 249, nil, 883, 249, 249, nil},
			{598, 598, nil, 615, 598, 598, nil, 615, nil},
			{571, 571, 476, 598, 571, 571, 476, 598, 834}
		}
	end

	local bots = waves[arena_next_wave]
	arena_final_wave = arena_next_wave >= #waves
	arena_current_wave = arena_next_wave
	arena_next_wave = nil
	arena_left = false

	-- Close doors before the first wave.
	if (arena_current_wave == 1) then 
		change_obstacle_state("Arena-AccessTrapdoor", "closed")
		change_obstacle_state("Arena-RingEntrance", "closed")
	end

	-- Initialize counter of bot.
	arena_remaining_bots = 0

	-- Teleport the bots into battlefield.
	for num,bot in pairs(bots) do
		create_droid("Arena-Teleport"..num, bot)
		arena_remaining_bots = arena_remaining_bots + 1
	end

	-- Display the number of the wave.
	if (not arena_final_wave) then
		display_big_message(_"Wave "..arena_current_wave)
	else
		display_big_message(_"Final Wave - Take care!")
	end
