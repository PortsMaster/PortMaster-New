<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "http://www.asahi-net.or.jp/~cs8k-cyu/bulletml/bulletml.dtd">

<bulletml type="vertical"
          xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">

<action label="topmove">
<changeSpeed>
 <speed>0.4+$rand*0.3</speed>
 <term>1</term>
</changeSpeed>
<wait>50+$rand*30</wait>
<changeSpeed>
 <speed>0</speed>
 <term>60</term>
</changeSpeed>
<wait>120+$rand*30</wait>
<changeSpeed>
 <speed>-0.3</speed>
 <term>60</term>
</changeSpeed>
</action>

</bulletml>
